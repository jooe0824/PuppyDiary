<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.puppydiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KgTab"/>
        <activity android:name=".CalendarTab"/>
        <activity android:name=".accountmenu.MoneyCalendar"/>
        <activity android:name=".accountmenu.MoneyTab"/>
        <activity android:name=".KgPopup"/>
        <activity android:name=".MypuppyTab"/>
        <activity android:name=".SetPuppy"/>
        <activity android:name=".CalendarDetail"/>

        <activity android:name=".accountmenu.MoneyEdit"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>